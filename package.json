{
  "name": "flossstock",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build-docker": "DOCKER_BUILD=true vite build --verbose",
    "docker-build": "docker build . -t $USER/league-page:latest",
    "docker-run": "npm run docker-build && docker run -p 3000:3000 $USER/league-page:latest",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "echo 'skip library packaging'",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "sideEffects": ["**/*.css"],
  "engines": { "node": ">=18.17 <23" },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@smui/button": "^8.0.0-beta.3",
    "@smui/common": "^8.0.3",
    "@smui/data-table": "^8.0.0-beta.3",
    "@smui/dialog": "^8.0.0-beta.3",
    "@smui/drawer": "^8.0.0-beta.3",
    "@smui/icon-button": "^8.0.0-beta.3",
    "@smui/linear-progress": "^8.0.0-beta.3",
    "@smui/list": "^8.0.0-beta.3",
    "@smui/paper": "^8.0.0-beta.3",
    "@smui/tab": "^8.0.0-beta.3",
    "@smui/tab-bar": "^8.0.0-beta.3",
    "@smui/textfield": "^8.0.0-beta.3",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-netlify": "^5.1.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/adapter-vercel": "^5.5.3",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "sass": "^1.90.0",
    "smui-theme": "^8.0.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.0.0",
    "unplugin-icons": "^22.2.0",
    "vite": "^7.0.4"
  },
  "keywords": ["svelte"],
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@vercel/analytics": "^1.4.1",
    "cheerio": "^1.1.2",
    "node-fetch": "^3.3.2"
  },
  "description": "Everything you need to build a Svelte library, powered by [`sv`](https://npmjs.com/package/sv).",
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezer/flossstock.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davezer/flossstock/issues"
  },
  "homepage": "https://github.com/davezer/flossstock#readme"
}
