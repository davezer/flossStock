{
  "name": "flossstock",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "svelte-kit preview",
    "dev:kit": "nodemon -e svelte,js,ts,css,html,svg -w src -w static -w db -x \"vite build\"",
    "dev:cf": "wrangler pages dev .svelte-kit/cloudflare --persist-to .wrangler/state",
    "dev:pages": "npm-run-all -p dev:kit dev:cf",
    "preview:pages": "vite build && wrangler pages dev .svelte-kit/cloudflare",
    "deploy": "vite build && wrangler pages deploy .svelte-kit/cloudflare --project-name threadindex",
    "db:schema:local": "wrangler d1 execute DB --local --file=./db/schema.sql",
    "db:seed:local": "wrangler d1 execute DB --local --file=./db/seed.dmc.sql",
    "db:schema:remote": "wrangler d1 execute DB --remote --file=./db/schema.sql",
    "db:seed:remote": "wrangler d1 execute DB --remote --file=./db/seed.dmc.sql",
    "db:reset:local": "powershell -NoProfile -Command \"if (Test-Path .wrangler\\state\\v3\\d1) { Remove-Item -Recurse -Force .wrangler\\state\\v3\\d1 }; exit 0\" && npm run db:schema:local && npm run db:seed:local",
    "tail": "wrangler pages deployment tail --project-name threadindex"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "engines": {
    "node": ">=18.17 <23"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@smui/button": "^8.0.0-beta.3",
    "@smui/common": "^8.0.3",
    "@smui/data-table": "^8.0.0-beta.3",
    "@smui/dialog": "^8.0.0-beta.3",
    "@smui/drawer": "^8.0.0-beta.3",
    "@smui/icon-button": "^8.0.0-beta.3",
    "@smui/linear-progress": "^8.0.0-beta.3",
    "@smui/list": "^8.0.0-beta.3",
    "@smui/paper": "^8.0.0-beta.3",
    "@smui/tab": "^8.0.0-beta.3",
    "@smui/tab-bar": "^8.0.0-beta.3",
    "@smui/textfield": "^8.0.0-beta.3",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/adapter-netlify": "^5.1.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/adapter-vercel": "^5.5.3",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.10.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "sass": "^1.90.0",
    "smui-theme": "^8.0.3",
    "svelte": "^5.43.5",
    "svelte-check": "^4.3.3",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.0.0",
    "unplugin-icons": "^22.2.0",
    "vite": "^7.0.4"
  },
  "keywords": [
    "svelte"
  ],
  "dependencies": {
    "@lucia-auth/adapter-sqlite": "3.0.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@vercel/analytics": "^1.4.1",
    "blueimp-md5": "^2.19.0",
    "cheerio": "^1.1.2",
    "lucia": "3.2.2",
    "node-fetch": "^3.3.2",
    "pdfjs-dist": "^5.4.394",
    "scrypt-js": "^3.0.1",
    "spark-md5": "^3.0.2"
  },
  "description": "Everything you need to build a Svelte library, powered by [`sv`](https://npmjs.com/package/sv).",
